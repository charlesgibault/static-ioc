<?xml version="1.0" encoding="UTF-8"?>
<beans 	xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:p="http://www.springframework.org/schema/p"
		xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	<!-- class with constructor args -->
	<bean id="importedBean" class="examples.ExampleBean">
  		<constructor-arg index="0"><value>00</value></constructor-arg>
  		<constructor-arg index="1"><value>10</value></constructor-arg>
  		<property name="nation" value="import" />
	</bean>
	
	<bean id="realAbstract" parent="importedAbstract">
		<property name="type" value="Cold hard concrete bean" />
	</bean>
	
	<bean id="importedAbstract" parent="importedBean" abstract="true" class="examples.ExampleBean">
		<property name="address" value="Somewhere over the rainbow" />
	</bean>
	
	<bean id="cycleChildren" parent="cycleParent" class="children.Bean">
	<!-- 	<constructor-arg index="2"><value>45</value></constructor-arg> -->
		<property name="name" value="cycleChildren" />
	</bean>
	
	<bean id="cycleGrandParent" parent="realAbstract" class="grandParent.Bean">
	<!-- 	<constructor-arg index="1"><value>1000</value></constructor-arg> -->
		<property name="name" value="cycleGrandParent" />
		<property name="age" value="old" />	
	</bean>
	
	<bean id="cycleParent" parent="cycleGrandParent" class="parent.Bean">
		<property name="age" value="40" />	
	</bean>
	
	<!-- Cyclic import that should be resolved -->
	<import resource="test.xml"/>	
</beans>